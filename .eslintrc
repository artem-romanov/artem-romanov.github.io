{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": false,
    "react/display-name": [false],
    "import/prefer-default-export": 0,
    "semi": 2
  },
  "globals": {
    "graphql": false
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended"
  ]
}